# AWS Configuration
AWS_REGION=us-east-1
QUEUE_URL=https://sqs.us-east-1.amazonaws.com/123456789012/image-processing-queue
S3_BUCKET=clore-processed-images-prod

# Database Configuration
DB_HOST=your-db-host.amazonaws.com
DB_PORT=5432
DB_NAME=your_database
DB_USER=your_username
DB_PASSWORD=your_password
DB_CONNECTION_STRING=postgresql://your_username:your_password@your-db-host.amazonaws.com:5432/your_database

# Processing Configuration
BATCH_SIZE=10
MAX_RETRIES=3
VISIBILITY_TIMEOUT=900
DEVICE=cuda
MAX_GPU_MEMORY=20

# S3 Configuration
S3_KEY_PREFIX=processed
S3_MAX_RETRIES=3

# Logging Configuration
LOG_LEVEL=INFO